<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
      http://www.liquibase.org/xml/ns/pro 
      http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">
    <changeSet  author="Kurnasevich"  id="t1">
        <createTable tableName="booking">
            <column  name="id"  type="INTEGER">
                <constraints  nullable="false"  primaryKey="true"  unique="true"/>
            </column>
            <column  name="passenger_name"  type="VARCHAR(255)" />
            <column  name="passenger_contact_number"  type="INTEGER" />
            <column  name="pickup_time"  type="DATE" />
            <column  name="asap"  type="BOOLEAN" />
            <column  name="waiting_time"  type="INTEGER" />
            <column  name="number_of_passengers"  type="INTEGER" />
            <column  name="price"  type="DOUBLE" />
            <column  name="rating"  type="FLOAT" />
            <column  name="created_on"  type="DATE" />
            <column  name="last_modified_on"  type="DATE" />
        </createTable>
    </changeSet>
</databaseChangeLog>